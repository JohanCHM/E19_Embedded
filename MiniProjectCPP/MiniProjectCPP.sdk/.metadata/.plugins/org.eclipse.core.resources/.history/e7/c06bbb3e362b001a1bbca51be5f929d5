{
   	int *p;	// properties.
//	uint8_t imgMatrix[100][100];	// Image

	VectorArray100 imgMatrix;
	VectorArray100 imgIx;
	VectorArray100 imgIy;

	initKernels();

	p = ReadImgProperties((char *)"mod5.bmp");
